{
  "name": "vue2vis",
  "version": "0.0.2",
  "description": "Vue2 component that helps with Visjs interaction",
  "jsnext": "dist/vue2vis.js",
  "main": "dist/vue2vis.common.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "purge": "rm -rf dist/",
    "compile": "rollup -c",
    "build": "npm run purge && npm run compile",
    "lint": "eslint src/* test/*",
    "beforePublish": "npm run lint && npm run build"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "author": "Alex Couturon <infocontact.alex@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vis",
    "visjs"
  ],
  "dependencies": {
    "lodash": "^4.16.4",
    "vis": "^4.20.1",
    "vue": "latest"
  },
  "devDependencies": {
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-import": "^1.16.0",
    "node-sass": "^3.10.1",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-vue": "^2.2.6"
  }
}
